      "hidden": true,
      "binaryDir": "${sourceDir}/out/build/${presetName}",
      "installDir": "${sourceDir}/out/install/${presetName}",
      "cacheVariables": {
        "LIBBNDL_BUILD_TOOLS": "ON"
      }
    },
    {
      "name": "ui",
      "hidden": true,
      "inherits": "Qt",
      "cacheVariables": {
        "LIBBNDL_BUILD_UI": "ON",
        "LIBBNDL_INSTALL_UI": "ON"
      }
    },
